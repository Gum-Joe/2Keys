{
  "name": "twokeys-server",
  "version": "0.5.1",
  "description": "Server for 2Keys",
  "main": "lib/index.js",
  "repository": "https://github.com/Gum-Joe/2Keys",
  "author": "Gum-Joe",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "compile": "tsc -p ./tsconfig.json",
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register test/**/*.ts",
    "coverage": "cross-env NODE_ENV=test nyc mocha test/**/*.ts",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov",
      "json"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bin": {
    "2Keys": "./bin/2Keys"
  },
  "keywords": [
    "twokeys",
    "2Keys",
    "2nd",
    "keyboard",
    "Taran",
    "LTT",
    "2nd keyboard",
    "macros",
    "raspberry",
    "pi",
    "raspberry pi",
    "python",
    "evdev",
    "autohotkey"
  ],
  "dependencies": {
    "adm-zip": "^0.4.14",
    "body-parser": "^1.18.3",
    "chalk": "^4.0.0",
    "express": "^4.16.3",
    "inquirer": "^7.0.0",
    "mkdirp": "^1.0.4",
    "mustache": "^4.0.1",
    "node-addon-api": "^2.0.1",
    "progress": "^2.0.1",
    "request": "^2.88.0",
    "tslint": "^6.1.1",
    "typescript": "^3.7.2",
    "yaml": "^1.9.2",
    "yargs": "^15.3.1",
    "snyk": "^1.316.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.33",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^7.0.2",
    "@types/mustache": "^4.0.1",
    "@types/node": "^13.13.2",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-fs": "^2.0.0",
    "codecov": "^3.6.5",
    "cross-env": "^7.0.2",
    "mocha": "^7.1.1",
    "node-gyp": "^6.1.0",
    "nyc": "^15.0.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.9.0"
  },
  "snyk": true
}
