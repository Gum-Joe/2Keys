{
  "name": "@twokeys/server",
  "version": "1.0.0",
  "description": "Server for 2Keys",
  "main": "lib/index.js",
  "repository": "https://github.com/Gum-Joe/2Keys",
  "author": "Gum-Joe",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "compile": "tsc -p tsconfig.build.json",
    "lint": "eslint --ext .ts .",
    "test": "cross-env NODE_ENV=test mocha -r ts-node/register test/**/*.ts",
    "coverage": "cross-env NODE_ENV=test nyc mocha test/**/*.ts",
    "coverage:upload": "codecov",
    "docs": "typedoc --ignoreCompilerErrors"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov",
      "json"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "bin": {
    "2Keys": "./bin/2Keys"
  },
  "keywords": [
    "twokeys",
    "2Keys",
    "2nd",
    "keyboard",
    "Taran",
    "LTT",
    "2nd keyboard",
    "macros",
    "raspberry",
    "pi",
    "raspberry pi",
    "python",
    "evdev",
    "autohotkey"
  ],
  "dependencies": {
    "@twokeys/core": "^1.0.0",
    "adm-zip": "^0.4.14",
    "bindings": "^1.3.0",
    "body-parser": "^1.18.3",
    "body-parser-json": "^1.9.2",
    "chalk": "^2.4.1",
    "express": "^4.16.3",
    "inquirer": "^7.0.0",
    "js-yaml": "^3.12.0",
    "mkdirp": "^1.0.0",
    "mustache": "^3.1.0",
    "node-addon-api": "^1.7.1",
    "node-windows": "^0.1.14",
    "progress": "^2.0.1",
    "request": "^2.88.0",
    "request-progress": "^3.0.0",
    "tslint": "^5.20.1",
    "typescript": "^3.9.2",
    "yaml": "^1.0.0-rc.8",
    "yargs": "^13.1.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.32",
    "@types/chai": "^4.2.11",
    "@types/chai-as-promised": "^7.1.2",
    "@types/inquirer": "^6.5.0",
    "@types/mocha": "^5.2.7",
    "@types/mustache": "^0.8.32",
    "@types/node": "^10.14.19",
    "@types/supertest": "^2.0.8",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-fs": "^2.0.0",
    "codecov": "^3.6.5",
    "cross-env": "^7.0.2",
    "mocha": "^6.2.2",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "ts-node": "^8.5.2",
    "typedoc": "^0.17.4"
  }
}
